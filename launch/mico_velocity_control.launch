<launch>
    <!-- Load joint controller configurations from YAML file to parameter server -->
    <rosparam file="$(find mico_hardware)/config/controllers.yaml" command="load"/>
    <node pkg="mico_hardware" 
          name="mico_hardware" 
          type="mico_hardware" 
          output="screen"/>
    <node pkg="controller_manager"
          type="spawner"
          args="vel_j1_controller 
          vel_j2_controller
          vel_j3_controller
          vel_j4_controller 
          vel_j5_controller 
          vel_j6_controller 
          vel_f1_controller 
          vel_f2_controller 
          joint_state_controller 
          velocity_joint_mode_controller"
          name="controller_manager"
          ns="ada" />

</launch>
